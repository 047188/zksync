#!/bin/bash

DIR=$KEY_DIR$BLOCK_SIZE_CHUNKS
echo "Checking keys in $DIR"
mkdir -p $DIR
#cd $KEY_DIR

for i in $KEY_FILES; do
    if ! [ -f $DIR/$i ]; then
        echo "Downloading file $SPACE_URL/$i"
        axel -o $DIR/$i -a $SPACE_URL/$i 2>&1
        #curl -o $KEY_DIR/$i $SPACE_URL/$i 2>&1
    fi
done