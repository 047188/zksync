{
  "name": "sync-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn gen_config && yarn update_zksync.js && vue-cli-service serve --port=7000",
    "build": "yarn gen_config && yarn update_zksync.js && vue-cli-service build",
    "gen_config": "f ../../bin/.gen_js_config > src/env-config.js",
    "update_zksync.js": "cd ../../../zksync.js && yarn build && cd ../zksync/js/explorer && yarn upgrade zksync.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.0.4",
    "core-js": "^3.3.2",
    "ethers": "^4.0.39",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "v-clipboard": "^2.2.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-timers": "^2.0.4",
    "zksync": "^0.3.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
}
