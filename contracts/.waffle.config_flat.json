{
    "sourcesPath": "./flat",
    "targetPath": "./flat_build",
    "compilerOptions": {
        "optimizer": {
            "enabled": true,
            "runs": 200,
            "details": {
                "peephole": true,
                "jumpdestRemover": true,
                "orderLiterals": true,
                "deduplicate": true,
                "cse": true,
                "constantOptimizer": true,
                "yul": false
            }
        }
    }
}
